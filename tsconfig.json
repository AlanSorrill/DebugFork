{
    "compilerOptions": {
        "jsx": "react",
        "module": "commonjs",
        "target": "ES2016",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "build",
        "skipLibCheck": true,
        "declaration": true,
        "declarationMap": true,
        "declarationDir": "declarations",
        "strictNullChecks": false,
        "downlevelIteration": true
    },
    "include": ["src"],
    "exclude": [ "node_modules"],
    "lib": ["es2015"],
    
  }